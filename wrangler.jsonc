{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "ovhtracker",
  "main": "src/index.ts",
  "compatibility_date": "2026-02-04",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  // make sure you have domain configured in cloudflare, if not, remove this section
  "routes": [
    {
      "pattern": "ovhtracker.lmpt.in",
      "zone_name": "lmpt.in",
      "custom_domain": true
    }
  ],
  "triggers": {
    "crons": ["0,30 * * * *"]
  }
}